<div class="container">
  <% if session[:user_id] %>
    <h1>All surveys taken by <%= current_user.first_name + " " + current_user.last_name %></h1>
    <table>
      <tr>
        <th>Num</th>
        <th>Subject</th>
        <th>Question</th>
      </tr>
      <% @surveys.each_with_index do |s,i| %>
      <tr>
        <td><%= (i+1).to_s %></td>
        <td><%= s.subject %></td>
        <td><%= s.question %></td>
      </tr>
      <% end %>
    </table><br>
  <% else %>
    <h1>Please log in</h1>
  <% end %>
  <table>
    <tr>
      <th>Username</th>
      <th>Email</th>
    </tr>
    <% @users.each do |u| %>
    <tr>
      <td><%= u.username %></td>
      <td><%= u.email %></td>
    </tr>
    <% end %>
  </table>

</div>